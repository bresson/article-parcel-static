{
  "name": "FE-Boilerplate",
  "version": "1.0.0",
  "description": "Front end boilerplate with Parcel, Pug and PostCSS forked from https://github.com/Blightwidow/article-parcel-static",
  "main": "index.js",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "ie 11",
    "last 2 versions"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "parcel build src/index.pug",
    "start": "parcel serve src/index.pug",
    "watch": "parcel src/index.pug",
    "lint": "echo \"No lint\" && exit 1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.2",
    "parcel-bundler": "^1.10.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-preset-env": "^6.4.0",
    "posthtml-pug": "^1.0.2",
    "pug": "^2.0.3",
    "whatwg-fetch": "^3.0.0"
  }
}
