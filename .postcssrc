{
  "plugins": {
    "autoprefixer": {
      "browsers": "last 2 versions, > 1%"
    },
    "postcss-preset-env": {
      "stage": 3,
      "features": {
        "nesting-rules": true,
        "case-insensitive-attributes": true,
        "hexadecimal-alpha-notation": true,
        "place-properties": true
      }
    },
  }
}
